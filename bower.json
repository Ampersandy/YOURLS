{
  "name": "YOURLS",
  "private": true,
  "dependencies": {
    "jquery": "~2.1.0",
    "bootstrap": "~3.1.1",
    "font-awesome": "~4.0.3",
    "flag-sprites": "~1.1.0",
    "bootstrap-social": "~4.2.0",
    "zeroclipboard": "~1.3.2"
  },
  "exportsOverride": {
    "jquery": {
      "js": "dist/jquery.min.js"
    },
    "bootstrap": {
      "less": "less/*.less",
      "js": "dist/js/*.min.js"
    },
    "font-awesome": {
      "less": "less/*.less",
      "fonts": "fonts/fontawesome-webfont.*"
    },
    "flag-sprites": {
      "less": "less/*.less",
      "img": "dist/img/*"
    },
    "bootstrap-social": {
      "less": "*.less"
    },
    "zeroclipboard": {
      "js": ["ZeroClipboard.min.js", "ZeroClipboard.swf"]
    }
  }
}
